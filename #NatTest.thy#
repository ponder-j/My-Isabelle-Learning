theory NatTest
  imports Main
begin

value "(8::nat) div 3"
value "(6::nat) mod 4"
value "(2::nat) dvd 6"
value "(6::nat) dvd 2"
value "(2::nat) < 4"
value "Max {1::nat, 2, 4}"
value "sgn(-15::int)"

definition f :: "nat \<Rightarrow> nat"
  where "f x \<equiv> 2 * x + 1"

term f

definition h :: "nat \<Rightarrow> nat \<Rightarrow> nat"
  where "h x y \<equiv> x + y"

term h

end