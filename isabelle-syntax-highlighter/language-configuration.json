{
  "comments": {
    "blockComment": ["(*", "*)"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\\<lparr>", "\\<rparr>"],
    ["\\<lbrakk>", "\\<rbrakk>"],
    ["\\<langle>", "\\<rangle>"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["\\<lparr>", "\\<rparr>"],
    ["\\<lbrakk>", "\\<rbrakk>"],
    ["\\<langle>", "\\<rangle>"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["\\<lparr>", "\\<rparr>"],
    ["\\<lbrakk>", "\\<rbrakk>"],
    ["\\<langle>", "\\<rangle>"]
  ],
  "wordPattern": "\\\\?<[a-zA-Z_][a-zA-Z0-9_']*>|[a-zA-Z_][a-zA-Z0-9_']*",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(proof|begin|where)\\s*$",
    "decreaseIndentPattern": "^\\s*(qed|end)\\s*$"
  }
}