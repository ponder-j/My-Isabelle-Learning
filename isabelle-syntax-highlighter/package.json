{
  "name": "isabelle-syntax-highlighter",
  "displayName": "Isabelle Syntax Highlighter",
  "description": "Syntax highlighting for Isabelle theorem prover files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "isabelle",
    "theorem prover",
    "syntax highlighting",
    "formal verification"
  ],
  "author": "Isabelle Syntax Highlighter Team",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "isabelle",
        "aliases": [
          "Isabelle",
          "isabelle"
        ],
        "extensions": [
          ".thy"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "isabelle",
        "scopeName": "source.isabelle",
        "path": "./syntaxes/isabelle.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/isabelle-syntax-highlighter.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/isabelle-syntax-highlighter/issues"
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  },
  "homepage": "https://github.com/your-username/isabelle-syntax-highlighter#readme"
}
